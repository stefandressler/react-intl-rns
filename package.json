{
  "name": "react-intl-rns",
  "version": "0.1.0",
  "description": "React Intl namespaced keys with fallback from specific to general",
  "author": "Stefan Dressler",
  "repository": {
    "type": "git",
    "url": "https://github.com/stefandressler/react-intl-rns.git"
  },
  "keywords": [
    "react",
    "react-intl"
  ],
  "scripts": {
    "postinstall": "npm run cab",
    "clean": "rimraf coverage lib out",
    "cab": "npm run compile && npm run build-umd && npm run build-min",
    "compile": "rimraf lib && babel src --ignore __tests__ --out-dir lib",
    "build-umd": "cross-env NODE_ENV=production webpack src/index.jsx out/react-intl-rns.js --output-library react-intl-rns --output-library-target umd",
    "build-min": "cross-env NODE_ENV=production webpack -p src/index.jsx out/react-intl-rns.min.js --output-library react-intl-rns --output-library-target umd",
    "eslint": "eslint . --ext=js --ext=jsx",
    "version": "./node_modules/.bin/standard-version",
    "release": "./scripts/release.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "main": "main.js",
  "dependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "^3.1.1",
    "eslint": "^3.11.1",
    "eslint-plugin-rapid7": "^5.0.1",
    "eslint-plugin-react": "^6.7.1",
    "eslint-plugin-sort-class-members": "^1.1.1",
    "react": "^15.4.0",
    "react-intl": "^2.1.5",
    "rimraf": "^2.5.4",
    "standard-version": "^2.4.0",
    "webpack": "^1.13.2"
  },
  "license": "MIT"
}
